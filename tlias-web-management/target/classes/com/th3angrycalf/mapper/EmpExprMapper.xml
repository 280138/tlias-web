<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.th3angrycalf.mapper.EmpExprMapper">

    <!--批量插入员工工作经历的信息 -->
    <!--separator 小于 open/close -->
    <insert id="insertBatch">
        insert into emp_expr(emp_id, begin, end, company, job) values
        <foreach collection="exprList" item="expr666" separator=",">
            (#{expr666.empId},#{expr666.begin},#{expr666.end},#{expr666.company},#{expr666.job})
        </foreach>
    </insert>

    <!--根据ID删除员工信息 -->
    <delete id="deleteByEmpIds">
        delete from emp_expr where emp_id in
        <foreach collection="empIds" item="empId" open="(" close=")" separator=",">
            #{empId}
        </foreach>
    </delete>
</mapper>